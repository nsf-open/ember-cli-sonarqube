{
  "name": "@nsf-open/ember-cli-sonarqube",
  "version": "1.2.0",
  "description": "Sonarqube analysis integration for Ember projects.",
  "repository": {
    "url": "https://github.com/nsf-open/ember-cli-sonarqube.git",
    "directory": "packages/ember-cli-sonarqube"
  },
  "bin": {
    "sonar": "./sonar.js"
  },
  "scripts": {
    "test": "echo \"Tests are specificed at the workspace level\" && exit 0",
    "release": "release-it"
  },
  "keywords": [
    "ember-addon"
  ],
  "author": "National Science Foundation",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "ember-cli-babel": "^7.26.11",
    "execa": "^5.1.1",
    "glob": "^7.2.3",
    "ora": "^5.4.1",
    "properties-reader": "^2.2.0",
    "sonarqube-scanner": "^2.8.1",
    "xmldom": "^0.6.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^2.0.1",
    "@types/yargs": "^17.0.10",
    "release-it": "^14.14.3"
  },
  "publishConfig": {
    "registry":"https://npm.pkg.github.com"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  }
}
